   <div class="main-context" id="main-context" style="background: url(bg.jpg);">
        <section id="play-main">
          <h3 style="text-align: center;" id="score"></h3>
          
          <p style="text-align: center;" id="lives"></p>
          
          <div style="text-align: center;" id="display">
          </div>

          <div id="invisible_text_data">
                       <h1 class="array_text">{{words}}</h1>
          </div>

          <div class="image">
            <img src="1.png" class="image-type" id="one" alt="main image">
            <img src="2.png" class="image-type" id="two" alt="main image">
            <img src="3.png" class="image-type" id="three" alt="main image">
            <img src="4.png" class="image-type" id="four" alt="main image">
            <img src="5.png" class="image-type" id="five" alt="main image">
            <img src="6.png" class="image-type" id="six" alt="main image">
            <img src="7.png" class="image-type" id="seven" alt="main image">

   
          </div>
  
          <div id="buttons">
          </div>  
  
        </section>
      

        <section id="content-part" class="hidden_done">
        
          <div class="main-context" id="main-context" style="background: url(play_done.jpg);">
              <div id="score_done">
          
              </div>
                <div class="play_done_button" id="play_done_button">
              <button id="go_main" class="go-main">Go to main</button>
              <button onclick=location.reload() id="play_again" class="play-again">Play agian</button>
            </div>
          </div>

        </section>
</div>
